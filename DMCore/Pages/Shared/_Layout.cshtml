    <div class="container body-content">
        @RenderBody()
    </div>

    <footer id="footer" class="midnight-blue">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    &copy; @DateTime.Now.Year <a target="_blank" href=@configuration.GetSection("SiteSettings")["SiteDomainURL"] title="Free Deals Daily and More">@configuration.GetSection("SiteSettings")["SiteDomain"]</a>. All Rights Reserved.
                </div>
                <div class="col-sm-6">
                    <ul class="pull-right">
                        <li><a href="#">Home</a></li>
                        <li><a href="#">About Us</a></li>
                        <li><a href="#">Faq</a></li>
                        <li><a href="#">Contact Us</a></li>
